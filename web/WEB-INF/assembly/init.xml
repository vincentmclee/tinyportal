<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

  <bean id="portalInitialor" class="com.econcord.portal.PortalInitiator" init-method="init">
	<property name="tabPageService" ref="tabPageService" />
	<property name="widgetService" ref="widgetService" />
	<property name="widgetInstanceService" ref="widgetInstanceService" />
  </bean>
  
</beans>
